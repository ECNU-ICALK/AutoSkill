# Copy to .env before `docker compose up --build -d`

# Provider selection
AUTOSKILL_LLM_PROVIDER=internlm
AUTOSKILL_EMBEDDINGS_PROVIDER=qwen

# API keys
INTERNLM_API_KEY=
DASHSCOPE_API_KEY=
ZHIPUAI_API_KEY=
BIGMODEL_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Generic compatible endpoints (optional)
AUTOSKILL_GENERIC_API_KEY=
AUTOSKILL_GENERIC_LLM_URL=
AUTOSKILL_GENERIC_LLM_MODEL=
AUTOSKILL_GENERIC_EMBED_URL=
AUTOSKILL_GENERIC_EMBED_MODEL=

# Runtime options
AUTOSKILL_USER_ID=u1
AUTOSKILL_SKILL_SCOPE=all
AUTOSKILL_REWRITE_MODE=always
AUTOSKILL_EXTRACT_MODE=auto
AUTOSKILL_EXTRACT_TURN_LIMIT=1
AUTOSKILL_MIN_SCORE=0.4
AUTOSKILL_TOP_K=1

# Optional proxy settings
AUTOSKILL_PROXY_MODELS=
AUTOSKILL_PROXY_API_KEY=

# Startup offline maintenance
AUTOSKILL_AUTO_NORMALIZE_IDS=1
AUTOSKILL_AUTO_IMPORT_DIRS=
AUTOSKILL_AUTO_IMPORT_SCOPE=common
AUTOSKILL_AUTO_IMPORT_LIBRARY=
AUTOSKILL_AUTO_IMPORT_OVERWRITE=0
AUTOSKILL_AUTO_IMPORT_INCLUDE_FILES=1
AUTOSKILL_AUTO_IMPORT_MAX_DEPTH=6

